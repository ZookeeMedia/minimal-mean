{
  "name": "minimal-mean-server",
  "version": "1.0.0",
  "description": "Easy to learn and use full-stack TypeScript with Angular 2",
  "main": "index.js",
  "engines": {
    "node": "6.9.1",
    "npm": "3.10.8"
  },
  "scripts": {
    "start": "node index.js",
    "tsc": "tsc",
    "tsc:w": "tsc -w",
    "image:build": "tsc && docker build . -t duluca/minimal-mean-server",
    "image:run": "docker run -p 3000:3000 duluca/minimal-mean-server",
    "image:publish": "docker push duluca/minimal-mean-server"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/excellalabs/minimal-mean.git"
  },
  "keywords": [
    "angular",
    "typescript",
    "node"
  ],
  "author": "Doguhan Uluca",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/excellalabs/minimal-mean/issues"
  },
  "homepage": "https://github.com/excellalabs/minimal-mean#readme",
  "dependencies": {
    "body-parser": "^1.15.0",
    "cors": "^2.8.1",
    "dev-norms": "^1.3.6",
    "document-ts": "^2.0.0",
    "express": "^4.13.4",
    "lodash": "^4.11.1",
    "mongodb": "^2.2.22",
    "morgan": "^1.7.0"
  },
  "devDependencies": {
    "@types/body-parser": "0.0.33",
    "@types/errorhandler": "0.0.30",
    "@types/express": "^4.0.35",
    "@types/jasmine": "^2.5.41",
    "@types/lodash": "^4.14.37",
    "@types/mime": "0.0.29",
    "@types/mongodb": "^2.1.38",
    "@types/morgan": "^1.7.32",
    "typescript": "^2.2.2"
  }
}
